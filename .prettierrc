{
  "printWidth": 120,
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrderTypeScriptVersion": "5.0.0",
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "^react",
    "",
    "^next",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^~(/.*)$",
    "",
    "^(?!.*[.]css$)[./].*$",
    "",
    ".css$"
  ]
}
